{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    // Add type definitions for our Astro runtime.
    "types": ["astro/client"],

    "baseUrl": ".",
    "paths": {
      "$content/*": ["src/content/*"],
      "$components/*": ["src/components/*"],
      "$assets/*": ["src/assets/*"],
      "$styles/*": ["src/styles/*"],
      "$layouts/*": ["src/layouts/*"]
    }
  }
}
